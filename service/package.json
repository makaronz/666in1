{
  "name": "chatgpt-web-service",
  "version": "2.15.6",
  "private": false,
  "description": "ChatGPT Web Midjourney Proxy",
  "author": "ChenZhaoYu <ydlhero@gmail.com>",
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "express"
  ],
  "engines": {
    "node": "^16 || ^18 || ^19"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "node ./build/index.mjs",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml",
    "test": "jest",
    "test:unit": "jest --coverage",
    "test:integration": "jest --config tests/integration/jest.config.js",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config tests/e2e/jest.config.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1535.0",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "chatgpt": "^5.1.2",
    "dotenv": "^16.0.3",
    "esno": "^0.16.3",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "^2.0.6",
    "https-proxy-agent": "^5.0.1",
    "isomorphic-fetch": "^3.0.0",
    "md5": "^2.3.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^3.3.0",
    "socks-proxy-agent": "^7.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.35.0",
    "jest": "^29.5.0",
    "rimraf": "^4.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
